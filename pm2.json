{
  "apps": [
    {
      "script": "build/src/app.js",
      "instances": "1",
      "exec_mode": "cluster",
      "name": "primary",
      "instance_var": "INSTANCE_ID",
      "cron_restart": "7 * * * *",
      "env_production": {
        "name": "prod-primary",
        "PORT": 3333,
        "NODE_ENV": "production"
      },
      "env_development": {
        "name": "dev-primary",
        "PORT": 3334,
        "NODE_ENV": "development"
      }
    },
    {
      "script": "build/src/app.js",
      "instances": 3,
      "exec_mode": "cluster",
      "name": "replica",
      "instance_var": "INSTANCE_ID",
      "env_production": {
        "name": "prod-replica",
        "PORT": 3333,
        "NODE_ENV": "production"
      },
      "env_development": {
        "name": "dev-replica",
        "PORT": 3334,
        "NODE_ENV": "development"
      }
    }
  ]
}