    "knex": "^0.95.11",
    "mysql2": "^2.3.0",
    "puppeteer": "^10.4.0",
    "request": "^2.88.2",
    "run-script-os": "^1.1.6",
    "socket.io": "^4.3.0"
  },
  "devDependencies": {
