    "/public/access/status/{serviceName}": {
      "get": {
        "tags": [
          "Down Detector"
        ],
        "summary": "recuperar status atual de um serviço",
        "parameters": [
          {
            "name": "serviceName",
            "in": "path",
            "description": "nome do serviço do qual quer o status",
            "required": true,
            "type": "string"
          },
          {
            "name": "token",
            "in": "query",
            "description": "token do cliente para ter acesso aos informações",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "status recuperado com sucesso",
            "schema": {
              "$ref": "#/definitions/inline_response_200"
            }
          },
          "400": {
            "description": "campo inválido ou não informado",
            "schema": {
              "$ref": "#/definitions/inline_response_400"
            }
          },
          "401": {
            "description": "token não informado, invalido, ou o token não tem permissão para realizar a operação",
            "schema": {
              "$ref": "#/definitions/inline_response_401"
            }
          },
          "406": {
            "description": "cliente não está habilitado, necessário entrar em contato com o adminstrador do sistema",
            "schema": {
              "$ref": "#/definitions/inline_response_406"
            }
